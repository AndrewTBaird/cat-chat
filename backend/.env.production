# Server Configuration
PORT=8080
NODE_ENV=production

# Frontend URL for CORS - update with your production frontend URL
FRONTEND_URL=https://cat-chat-frontend-production.up.railway.app

# Database - Railway will auto-inject this
DATABASE_URL=${{Postgres.DATABASE_URL}}

# JWT Authentication - Set this in Railway dashboard
JWT_SECRET=your-production-secret-key-here
JWT_EXPIRES_IN=7d
